{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/not-found.js", "src/components/sportsEvents/EventContact.jsx", "src/components/parentToddler/TestimonialsSection.jsx", "src/components/parentToddler/BenefitsSection.jsx", "src/components/parentToddler/CTASection.jsx", "src/components/parentToddler/Footer.jsx", "src/components/parentToddler/Header.jsx", "src/components/parentToddler/HeroSection.jsx", "src/components/parentToddler/ProgramDetailsSection.jsx", "src/components/parentToddler/ProgramOverview.jsx", "src/app/services/parentToddler/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
